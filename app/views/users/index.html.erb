<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Nat</th>
      <th>Seed</th>
      <th>Gender</th>
      <th>Name</th>
      <th>Email</th>
      <th>Picture large</th>
      <th>Picture thumbnail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.nat %></td>
        <td><%= user.seed %></td>
        <td><%= user.gender %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.picture_large %></td>
        <td><%= user.picture_thumbnail %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= paginate @users %>
<%= page_entries_info @users %>
